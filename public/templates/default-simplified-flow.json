      "target": "experience-years",
      "label": "Next"
    },
    {
      "id": "experience-years-to-condition",
      "source": "experience-years",
      "target": "experience-level-condition",
      "label": "Evaluate Experience"
    },
    {
      "id": "condition-to-entry-summary",
      "source": "experience-level-condition",
      "target": "entry-level-summary",
      "sourceHandle": "A",
      "targetHandle": null,
      "label": "Entry Level"
    },
    {
      "id": "condition-to-mid-summary",
      "source": "experience-level-condition",
      "target": "mid-level-summary",
      "sourceHandle": "B",
      "targetHandle": null,
      "label": "Mid Level"
    },
    {
      "id": "condition-to-senior-summary",
      "source": "experience-level-condition",
      "target": "senior-level-summary",
      "sourceHandle": "C",
      "targetHandle": null,
      "label": "Senior Level"
    },
    {
      "id": "entry-summary-to-skills",
      "source": "entry-level-summary",
      "target": "skills",
      "label": "Next"
    },
    {
      "id": "mid-summary-to-skills",
      "source": "mid-level-summary",
      "target": "skills",
      "label": "Next"
    },
    {
      "id": "senior-summary-to-skills",
      "source": "senior-level-summary",
      "target": "skills",
      "label": "Next"
    },
    {
      "id": "skills-to-education-level",
      "source": "skills",
      "target": "education-level",
      "label": "Next"
    },
    {
      "id": "education-level-to-details",
      "source": "education-level",
      "target": "education-details",
      "label": "Next"
    },
    {
      "id": "education-details-to-languages",
      "source": "education-details",
      "target": "languages",
      "label": "Next"
    },
    {
      "id": "languages-to-additional",
      "source": "languages",
      "target": "additional-info",
      "label": "Next"
    },
    {
      "id": "additional-to-end",
      "source": "additional-info",
      "target": "end",
      "label": "Complete"
    }
  ],
  "variables": [
